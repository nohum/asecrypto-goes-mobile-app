<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="at.fhj.gaar.asecrypto.mobile" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".ui.ExampleListActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.ExampleDetailActivity"
            android:label="@string/title_example_detail"
            android:parentActivityName=".ui.ExampleListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="at.fhj.gaar.asecrypto.mobile.ExampleListActivity" />
        </activity>
    </application>

</manifest>
